<Window x:Class="ReservationSystem.AdminMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReservationSystem"
        mc:Ignorable="d"
        Title="AdminMainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="True">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Welcome!" IsEnabled="False"/>
                <MenuItem Header="Hotels" Click="Hotels_Click"/>
                <MenuItem Header="Users" Click="Users_Click"/>
                <MenuItem Header="Logout" Click="Logout_Click"/>
            </Menu>
            <Frame Name="MainContent" />
        </DockPanel>
    </Grid>
</Window>
